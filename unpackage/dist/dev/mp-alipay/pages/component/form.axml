<view><cu-custom vue-id="9c232326-1" bgColor="bg-gradual-pink" isBack="{{true}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">表单</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title">邮件</view><input placeholder="两字短标题" name="input"/></view><view class="cu-form-group"><view class="title">输入框</view><input placeholder="三字标题" name="input"/></view><view class="cu-form-group"><view class="title">收货地址</view><input placeholder="统一标题的宽度" name="input"/></view><view class="cu-form-group"><view class="title">收货地址</view><input placeholder="输入框带个图标" name="input"/><text class="cuIcon-locationfill text-orange"></text></view><view class="cu-form-group"><view class="title">验证码</view><input placeholder="输入框带个按钮" name="input"/><button class="cu-btn bg-green shadow">验证码</button></view><view class="cu-form-group"><view class="title">手机号码</view><input placeholder="输入框带标签" name="input"/><view class="cu-capsule radius"><view class="cu-tag bg-blue ">+86</view><view class="cu-tag line-blue">中国大陆</view></view></view><view class="cu-form-group margin-top"><view class="title">普通选择</view><picker value="{{index}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+(index>-1?picker[index]:'禁止换行，超出容器部分会以 ... 方式截断')+''}}</view></picker></view><view class="cu-form-group"><view class="title">时间选择</view><picker mode="time" value="{{time}}" start="09:01" end="21:01" data-event-opts="{{[['change',[['TimeChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+time+''}}</view></picker></view><view class="cu-form-group"><view class="title">日期选择</view><picker mode="date" value="{{date}}" start="2015-09-01" end="2020-09-01" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+date+''}}</view></picker></view><view class="cu-form-group margin-top"><view class="title">开关选择</view><switch class="{{(switchA?'checked':'')}}" checked="{{switchA?true:false}}" data-event-opts="{{[['change',[['SwitchA',['$event']]]]]}}" onChange="__e"></switch></view><view class="cu-form-group"><view class="title">定义颜色</view><switch class="{{(('red')+' '+(switchB?'checked':''))}}" checked="{{switchB?true:false}}" color="#e54d42" data-event-opts="{{[['change',[['SwitchB',['$event']]]]]}}" onChange="__e"></switch></view><view class="cu-form-group"><view class="title">定义图标</view><switch class="{{(('switch-sex')+' '+(switchC?'checked':''))}}" checked="{{switchC?true:false}}" data-event-opts="{{[['change',[['SwitchC',['$event']]]]]}}" onChange="__e"></switch></view><radio-group data-event-opts="{{[['change',[['RadioChange',['$event']]]]]}}" class="block" onChange="__e"><view class="cu-form-group margin-top"><view class="title">单选操作(radio)</view><radio class="{{(radio=='A'?'checked':'')}}" checked="{{radio=='A'?true:false}}" value="A"></radio></view></radio-group><checkbox-group data-event-opts="{{[['change',[['CheckboxChange',['$event']]]]]}}" class="block" onChange="__e"><view class="cu-form-group margin-top"><view class="title">复选选操作(checkbox)</view><checkbox class="{{(checkbox[0].checked?'checked':'')}}" checked="{{checkbox[0].checked?true:false}}" value="A"></checkbox></view></checkbox-group><view class="cu-bar bg-white margin-top"><view class="action">图片上传</view><view class="action">{{''+imgList.length+'/4'}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block a:for="{{imgList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" onTap="__e"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchTap="__e"><text class="cuIcon-close"></text></view></view></block><block a:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" onTap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="cu-form-group margin-top"><view class="title">头像</view><view class="cu-avatar radius bg-gray"></view></view><view class="cu-form-group margin-top"><textarea maxlength="-1" disabled="{{modalName!=null}}" placeholder="多行文本输入框" data-event-opts="{{[['input',[['textareaAInput',['$event']]]]]}}" onInput="__e"></textarea></view><view class="cu-form-group align-start"><view class="title">文本框</view><textarea maxlength="-1" disabled="{{modalName!=null}}" placeholder="多行文本输入框" data-event-opts="{{[['input',[['textareaBInput',['$event']]]]]}}" onInput="__e"></textarea></view></form></view>