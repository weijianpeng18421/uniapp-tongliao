<view class="{{(('cu-modal')+' '+(modalStatus=='show'?'show':''))}}"><view class="cu-dialog"><view class="cu-bar modal-title-bg justify-end"><view class="content">{{name}}</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" onTap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="modal-content-bg padding-xl text-center"><view class="title">请在下面输入签名：</view><canvas class="mycanvas" canvas-id="mycanvas" data-event-opts="{{[['touchStart',[['touchstart',['$event']]]],['touchMove',[['touchmove',['$event']]]],['touchEnd',[['touchend',['$event']]]]]}}" onTouchStart="__e" onTouchMove="__e" onTouchEnd="__e"></canvas><view class="cu-bar justify-center"><button data-event-opts="{{[['tap',[['finish',['$event']]]]]}}" class="cu-btn modal-btn" onTap="__e">确定</button><button data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="cu-btn modal-btn" onTap="__e">清除</button></view></view></view></view>